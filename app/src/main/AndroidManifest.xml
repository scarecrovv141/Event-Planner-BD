<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.final_tausif">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.final_tausif.ShowEvents"></activity>
        <activity android:name="com.example.final_tausif.createPost" />
        <activity android:name="com.example.final_tausif.ShowOrgs" />
        <activity android:name="com.example.final_tausif.profile" />
        <activity android:name="com.example.final_tausif.orgReg" />
        <activity android:name="com.example.final_tausif.userReg" />
        <activity android:name="com.example.final_tausif.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>